QWidget {
    background-color: #748c85;
}

QLabel, QListWidget, QPushButton {
    background-color: #f2ca99;
    border-style: solid;
    border-width: 3px;
    border-color: #8c4f2b;
    border-radius: 5px;
    color: #0d0902;
}

QLabel {
    text-align: center;
    padding: 10px
}

QListWidget {
    padding: 10px;
}

QPushButton {
    font-weight: bold;
    text-decoration: underline;
}